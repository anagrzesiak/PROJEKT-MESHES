<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="473.0" prefWidth="699.0">
         <children>
            <TextField fx:id="inputField1" layoutX="231.0" layoutY="14.0" prefHeight="25.0" prefWidth="133.0" promptText="DENSITY(HORIZONTAL)" />
            <Button fx:id="exportButton" layoutX="596.0" layoutY="373.0" mnemonicParsing="false" onAction="#clickExport" prefHeight="25.0" prefWidth="89.0" text="EXPORT" />
            <Button fx:id="squareButton" layoutX="14.0" layoutY="52.0" mnemonicParsing="false" onAction="#clickSquare" prefHeight="51.0" prefWidth="89.0" text="MESH &#10;(SQUARES)" textAlignment="CENTER" />
            <Button fx:id="triangleButton" layoutX="14.0" layoutY="121.0" mnemonicParsing="false" onAction="#clickTriangle" prefHeight="51.0" prefWidth="89.0" text="MESH&#10;(TRIANGLES)&#10;" textAlignment="CENTER" />
            <Button fx:id="importButton" layoutX="596.0" layoutY="251.0" mnemonicParsing="false" onAction="#clickImport" prefHeight="25.0" prefWidth="89.0" text="IMPORT" />
            <TextField fx:id="inputField2" layoutX="79.0" layoutY="14.0" prefHeight="25.0" prefWidth="140.0" promptText="DENSITY(VERTICAL)" />
            <Canvas fx:id="canvas" height="403.0" layoutX="114.0" layoutY="52.0" width="471.0" />
            <TextField fx:id="inputHeight" layoutX="535.0" layoutY="14.0" prefHeight="25.0" prefWidth="150.0" promptText="HEIGHT" />
            <TextField fx:id="inputWidth" layoutX="374.0" layoutY="14.0" prefHeight="25.0" prefWidth="150.0" promptText="WIDTH" />
            <Button fx:id="refreshButton" layoutX="596.0" layoutY="109.0" mnemonicParsing="false" onAction="#clickRefresh" prefHeight="25.0" prefWidth="89.0" text="REFRESH" />
            <Button fx:id="rhombButton" layoutX="14.0" layoutY="192.0" mnemonicParsing="false" onAction="#tryRhomb" prefHeight="51.0" prefWidth="89.0" text="TRY RHOMB" />
            <Button fx:id="trapezeButton" layoutX="14.0" layoutY="263.0" mnemonicParsing="false" onAction="#tryTrapeze" prefHeight="51.0" prefWidth="89.0" text="TRY TRAPEZE" />
            <Button fx:id="pictureButton" layoutX="14.0" layoutY="334.0" mnemonicParsing="false" onAction="#setPictureButton" prefHeight="51.0" prefWidth="89.0" text="MESH &#10;PICTURE&#10;" textAlignment="CENTER" />
            <Button fx:id="colorsButton" layoutX="14.0" layoutY="404.0" mnemonicParsing="false" onAction="#clickColors" prefHeight="51.0" prefWidth="89.0" text="MESH&#10;COLORS" textAlignment="CENTER" />
         </children>
      </Pane>
   </children>
</GridPane>
